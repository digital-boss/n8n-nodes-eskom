{
  "name": "@digital-boss/n8n-nodes-eskom",
  "version": "0.1.2",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/digital-boss/n8n-nodes-eskom",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-boss/n8n-nodes-eskom.git"
  },
  "keywords": [
    "n8n",
    "workflow",
    "n8n-community-node-package",
    "digital boss"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && gulp",
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "nodelinter": "nodelinter",
    "version": "genversion -s -e nodes/version.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/credentials/EskomApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Eskom/Eskom.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^27.4.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "14.17.27",
    "@types/request-promise-native": "~1.0.15",
    "genversion": "^3.1.1",
    "gulp": "^4.0.0",
    "jest": "^27.4.7",
    "n8n-workflow": "~0.117.2",
    "nodelinter": "^0.1.9",
    "ts-jest": "^27.1.3",
    "tsc-watch": "^5.0.3",
    "tslint": "^6.1.2",
    "typescript": "~4.6.0"
  },
  "dependencies": {
    "@digital-boss/n8n-designpatterns": "0.1.8",
    "lodash.merge": "^4.6.2",
    "n8n-core": "~0.135.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
